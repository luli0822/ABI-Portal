{
  "name": "nextjs-accelerator",
  "version": "0.0.1",
  "description": "A Nextjs accelerator project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "fetch-data": "node contentful/save-data-to-json.js",
    "dev": "npm run fetch-data && next",
    "build": "npm run fetch-data && next build",
    "build:static": "npm run build && next export",
    "start": "next start",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "lint:js": "eslint --ext js,jsx ./ --max-warnings 0",
    "lint:js:fix": "eslint --ext js,jsx ./ --max-warnings 0 --fix",
    "lint:scss": "stylelint \"**/*.scss\" --syntax scss",
    "lint:scss:fix": "stylelint \"**/*.scss\" --syntax scss --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --quiet -c .storybook -o public/storybook"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.lblw.ca:2222/dxstudio/nextjs-accelerator.git"
  },
  "browserslist": [
    "last 2 versions",
    "> 1% in CA",
    "not dead"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://gitlab.lblw.ca/dxstudio/nextjs-accelerator#readme",
