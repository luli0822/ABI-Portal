    "mime": "^2.4.4",
    "next": "^9.1.7",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^0.17.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
